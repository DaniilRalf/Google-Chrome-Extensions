{
  "name": "Itwis extensional",
  "description": "Itwis extensional description",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "cookies"
  ],
  "host_permissions": [
    "https://thecode.media/extention/",
    "*://*/*"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ],


  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Itwis extensional",
    "default_popup": "index.html",
    "default_icon": {
      "16": "/4-01.png",
      "32": "/3-01.png",
      "48": "/2-01.png",
      "128": "/1-01.png"
    }
  },
  "icons": {
    "16": "/4-01.png",
    "32": "/3-01.png",
    "48": "/2-01.png",
    "128": "/1-01.png"
  }
}
